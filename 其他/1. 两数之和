class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<int> res;
        map<int,int> map;
        for(int i=0;i<nums.size();i++){
            if(map.count(target-nums[i])>0){
                res.push_back(i);
                res.push_back(map.find(target-nums[i])->second);
                return res;
            }
            map[nums[i]]=i;
        }
        return res;
    }
};
