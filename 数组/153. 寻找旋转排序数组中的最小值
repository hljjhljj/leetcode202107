class Solution {
public:
    int findMin(vector<int>& nums) {
        int left=0,right=nums.size()-1;//[left,right]
        while(left<right){
            int mid=(left+right)/2;
            if(nums[mid]<nums[right]) right=mid;
            if(nums[mid]>nums[right]) left=mid+1;
            //nums[mid]=nums[right]情况不存在
        }
        return nums[left];
    }
};

